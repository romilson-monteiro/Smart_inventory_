#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/c/Users/User/Desktop/Smart_inventory_/Server/node_modules/.pnpm/mqtt@5.10.1/node_modules/mqtt/build/bin/node_modules:/mnt/c/Users/User/Desktop/Smart_inventory_/Server/node_modules/.pnpm/mqtt@5.10.1/node_modules/mqtt/build/node_modules:/mnt/c/Users/User/Desktop/Smart_inventory_/Server/node_modules/.pnpm/mqtt@5.10.1/node_modules/mqtt/node_modules:/mnt/c/Users/User/Desktop/Smart_inventory_/Server/node_modules/.pnpm/mqtt@5.10.1/node_modules:/mnt/c/Users/User/Desktop/Smart_inventory_/Server/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/c/Users/User/Desktop/Smart_inventory_/Server/node_modules/.pnpm/mqtt@5.10.1/node_modules/mqtt/build/bin/node_modules:/mnt/c/Users/User/Desktop/Smart_inventory_/Server/node_modules/.pnpm/mqtt@5.10.1/node_modules/mqtt/build/node_modules:/mnt/c/Users/User/Desktop/Smart_inventory_/Server/node_modules/.pnpm/mqtt@5.10.1/node_modules/mqtt/node_modules:/mnt/c/Users/User/Desktop/Smart_inventory_/Server/node_modules/.pnpm/mqtt@5.10.1/node_modules:/mnt/c/Users/User/Desktop/Smart_inventory_/Server/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../mqtt@5.10.1/node_modules/mqtt/build/bin/sub.js" "$@"
else
  exec node  "$basedir/../../../../../mqtt@5.10.1/node_modules/mqtt/build/bin/sub.js" "$@"
fi
